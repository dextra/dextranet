<?xml version="1.0" encoding="UTF-8"?>
<engine>
	<template selector="#container_main">
		<form id="new_banner" method="post" enctype="multipart/form-data">
			<input type="text" id="bannerTitulo" name="bannerTitulo" placeholder="Digite o título do banner" /><br/>
			<input type="file" id="file" name="banner" /><br/>
			De: <input type="text" id="bannerDataInicio" name="dataInicio" size="15" />
			Até: <input type="text" id="bannerDataFim" name="dataFim" size="15" /><br/>
			<input type="button" value="Upload!" />
		</form>
	</template>
	<script>
	dextranet.banner.validaEFormataForm();
	$("#new_banner input[type=button]").click(dextranet.banner.novoBanner);
	</script>
</engine>