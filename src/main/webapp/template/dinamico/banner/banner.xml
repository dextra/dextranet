<?xml version="1.0" encoding="UTF-8"?>
<engine>
	<template selector="#banner" append="true">
		<div class="close">\/</div>
			{for banner in jsonArrayBanner}
				<img src="/s/banner/${banner.postObjectJson.id}"/>
			{/for}	
	</template>
	<script>
		$('#banner img:first').load(function() {
            dextranet.banner.mostrarBanners();
		});
	</script>
</engine>