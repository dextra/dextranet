<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://appengine.google.com/ns/1.0 http://googleappengine.googlecode.com/svn/branches/1.2.1/java/docs/appengine-web.xsd">

<!-- 	 APPLICATION PROD -->
<!--	<application>dextra-dextranet</application> -->
<!--	<version>prod</version> -->

<!-- 	 APPLICATION DEV -->
	<application>dev-dextranet</application>
 	<version>dev</version>

	<static-files>
		<include path="/**" />
		<exclude path="/index.html" />
	</static-files>

	<resource-files>
		<include path="/index.html" />
		<include path="/template/**" />
		<include path="/resources/**" />
	</resource-files>

	<sessions-enabled>false</sessions-enabled>
	<threadsafe>true</threadsafe>

</appengine-web-app>
